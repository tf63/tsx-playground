{
	"type": "module",
	"name": "tsx-playground",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"format": "biome format . && prettier .",
		"format:fix": "biome format . --write && prettier --write .",
		"check": "biome check . && prettier .",
		"check:fix": "biome check . --write && prettier --write .",
		"prepare": "lefthook install"
	},
	"dependencies": {
		"react": "19.2.3",
		"react-dom": "19.2.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@commitlint/cli": "19.8.1",
		"@types/node": "22.13.14",
		"@types/react": "19.2.3",
		"@types/react-dom": "19.2.3",
		"prettier": "3.6.2",
		"jsdom": "27.2.0",
		"lefthook": "1.13.0",
		"turbo": "2.7.2",
		"typescript": "5.9.2"
	},
	"packageManager": "pnpm@10.24.0"
}
